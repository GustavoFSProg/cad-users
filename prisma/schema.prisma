// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./devs.db"
}

model User {
  id       String   @id @default(uuid())
  name     String?
  email    String   @unique
  password String
  creatAt  DateTime @default(now())
}
